<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="biolaer.dk.biolaer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">

        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.HighscoreActivity" />
        <activity android:name=".Activities.EasyHsActivity" />
        <activity android:name=".Activities.HardHsActivity" />
        <activity android:name=".Activities.OptionsActivity" />
        <activity android:name=".Activities.QuestionsActivity" />
        <activity android:name=".Activities.EndActivity" />
        <activity android:name=".Activities.GameActivity" />
        <activity android:name=".Activities.LevelActivity" />

        <service
            android:name=".BusinessLogic.MusicService"
            android:enabled="true" />

        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>